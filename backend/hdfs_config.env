# HDFS Configuration Template
# Copy this file to .env and modify as needed

# HDFS Server Configuration
HDFS_HOSTNAME=quickstart.cloudera
HDFS_IP=192.168.1.56
HDFS_USER=cloudera

# Ports
WEBHDFS_PORT=50070
HDFS_PORT=8020

# Optional: Override automatic discovery
# Set this to 'true' to use the IP above instead of auto-discovery
FORCE_IP=false

# Connection timeout (seconds)
HDFS_TIMEOUT=10 